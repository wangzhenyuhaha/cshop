<resources
    xmlns:app="http://schemas.android.com/apk/res-auto"
    >
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
<!--        <item name="colorPrimary">@color/colorPrimary</item>-->
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorControlNormal">?android:attr/textColorPrimary</item>
<!--        <item name="colorAccent">@color/colorAccent</item>-->
    </style>

    <style name="divide_line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
        <item name="android:background">@color/divide_line</item>
    </style>

    <style name="LoadDialog" parent="Base.Theme.AppCompat.Dialog">
        <item name="windowNoTitle">true</item>
    </style>

    <style name="common_divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dimen_half</item>
        <item name="android:background">@color/color_E6E6E6</item>
    </style>

    <style name="dash_line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">2dp</item>
        <item name="android:background">@drawable/line_h</item>
        <item name="android:layerType">software</item>
    </style>

    <style name="dash_v_line">
        <item name="android:layout_width">3dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@drawable/line_v</item>
        <item name="android:layerType">software</item>
    </style>

    <style name="style_goods_menu">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawablePadding">@dimen/dimen_4</item>
        <item name="android:paddingLeft">@dimen/dimen_14</item>
        <item name="android:paddingRight">@dimen/dimen_14</item>
        <item name="android:textColor">@color/color_ffffff</item>
        <item name="android:textSize">@dimen/dimen_10</item>
        <item name="android:paddingTop">@dimen/dimen_10</item>
        <item name="android:paddingBottom">@dimen/dimen_10</item>
    </style>

    <style name="style_goods_category_title">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">@dimen/dimen_9</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">@color/color_333333</item>
        <item name="android:textSize">@dimen/dimen_13</item>
    </style>

    <style name="style_goods_category_indicator">
        <item name="android:layout_width">22dp</item>
        <item name="android:layout_height">2dp</item>
        <item name="android:layout_marginStart">@dimen/dimen_15</item>
        <item name="android:background">@color/color_3870EA</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="goods_group_edit_left">
        <item name="android:layout_width">80dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/color_333333</item>
        <item name="android:textSize">@dimen/dimen_13</item>
    </style>

    <style name="goods_group_edit_right">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@null</item>
        <item name="android:gravity">center_vertical|right</item>
        <item name="android:textColor">@color/color_333333</item>
        <item name="android:textColorHint">@color/color_999999</item>
        <item name="android:textSize">@dimen/dimen_13</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="goods_group_edit_container">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/dimen_14</item>
        <item name="android:orientation">vertical</item>
    </style>


    <!--  商品发布页 start  -->
    <style name="goods_publish_container">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
<!--        <item name="android:layout_marginLeft">@dimen/dimen_14</item>-->
<!--        <item name="android:layout_marginRight">@dimen/dimen_14</item>-->
        <item name="android:paddingLeft">@dimen/dimen_10</item>
        <item name="android:paddingRight">@dimen/dimen_10</item>
        <item name="android:layout_marginTop">@dimen/dimen_14</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="goods_publish_sub_container">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dimen_50</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="goods_publish_left">
        <item name="android:layout_width">100dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/color_333333</item>
        <item name="android:textSize">@dimen/dimen_13</item>
    </style>

    <style name="goods_publish_left_mark" parent="goods_publish_left">
        <item name="android:drawablePadding">@dimen/dimen_4</item>
        <item name="android:drawableStart">@mipmap/common_must_input</item>
    </style>

    <style name="goods_publish_right">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@null</item>
        <item name="android:gravity">center_vertical|right</item>
        <item name="android:textColorHint">@color/color_999999</item>
        <item name="android:textColor">@color/color_666666</item>
        <item name="android:textSize">@dimen/dimen_13</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="goods_publish_right_arrow" parent="goods_publish_right">
        <item name="android:drawablePadding">@dimen/dimen_11</item>
        <item name="android:drawableRight">@mipmap/common_arrow_right</item>
    </style>
    <!--  商品发布页 end -->


    <!--  商品规格设置页 start -->
    <style name="goods_spec_sub_container">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <style name="goods_spce_item_left">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingEnd">@dimen/dimen_5</item>
        <item name="android:textColor">@color/color_333333</item>
        <item name="android:textSize">@dimen/dimen_13</item>
    </style>
    <style name="goods_spce_item_left_mark" parent="goods_spce_item_left">
        <item name="android:drawableStart">@mipmap/common_must_input</item>
        <item name="android:drawablePadding">@dimen/dimen_5</item>
    </style>

    <style name="goods_spec_item_right_edt">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@null</item>
        <item name="android:ellipsize">end</item>
        <item name="android:inputType">numberDecimal</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColorHint">@color/color_999999</item>
        <item name="android:textColor">@color/color_999999</item>
        <item name="android:textSize">@dimen/dimen_12</item>
    </style>

    <style name="goods_spec_setting_container">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dimen_50</item>
        <item name="android:layout_marginStart">@dimen/dimen_14</item>
        <item name="android:layout_marginEnd">@dimen/dimen_14</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="goods_spec_setting_left">
        <item name="android:layout_width">130dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/color_333333</item>
        <item name="android:textSize">@dimen/dimen_13</item>
    </style>

    <style name="goods_spec_setting_right">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@null</item>
        <item name="android:paddingStart">@dimen/dimen_15</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColorHint">@color/color_999999</item>
        <item name="android:textColor">@color/color_333333</item>
        <item name="android:textSize">@dimen/dimen_13</item>
    </style>
    <!--  商品规格设置页 end -->


    <!--  发货方式 start -->
    <style name="goods_delivery_title">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:paddingStart">@dimen/dimen_10</item>
        <item name="android:textSize">@dimen/dimen_13</item>
        <item name="android:textColor">@color/color_333333</item>
        <item name="android:paddingEnd">@dimen/dimen_30</item>
        <item name="android:button">@drawable/selector_goods_delivery_checkbox</item>
    </style>

    <style name="goods_delivery_row_container">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dimen_50</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingStart">@dimen/dimen_14</item>
        <item name="android:paddingEnd">@dimen/dimen_14</item>
    </style>

    <style name="goods_delivery_row_left">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:button">@drawable/selector_goods_delivery_child_checkbox</item>
        <item name="android:paddingStart">@dimen/dimen_10</item>
        <item name="android:paddingEnd">@dimen/dimen_30</item>
        <item name="android:textColor">@color/color_333333</item>
        <item name="android:textSize">@dimen/dimen_13</item>
    </style>

    <style name="goods_delivery_edit_right" parent="goods_group_edit_right">
        <item name="android:inputType">numberDecimal</item>
    </style>

    <style name="goods_delivery_right_arrow" parent="goods_publish_right_arrow"/>

